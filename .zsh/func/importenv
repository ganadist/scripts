#!/usr/bin/env bash

IMPORT_ENV="PATH LD_LIBRARY_PATH"
IMPORT_ENV="$IMPORT_ENV GPG_AGENT_INFO SSH_AUTH_SOCK"
IMPORT_ENV="$IMPORT_ENV PYTHONSTARTUP"
IMPORT_ENV="$IMPORT_ENV GOPATH"
IMPORT_ENV="$IMPORT_ENV STUDIO_PROPERTIES ANDROID_HOME ANDROID_NDK_ROOT STUDIO_JDK"
IMPORT_ENV="$IMPORT_ENV FT2_SUBPIXEL_HINTING IBUS_ENABLE_SYNC_MODE"


echo $IMPORT_ENV | xargs systemctl --user import-environment
